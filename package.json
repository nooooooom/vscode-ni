{
  "name": "vscode-ni",
  "displayName": "vscode-ni",
  "description": "Using @antfu/ni via vscode extension.",
  "version": "0.0.2",
  "publisher": "nooooooom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nooooooom/vscode-ni.git"
  },
  "bugs": {
    "url": "https://github.com/nooooooom/vscode-ni/issues"
  },
  "keywords": [
    "package-manager",
    "npm",
    "yarn",
    "pnpm",
    "vscode",
    "vscode-extension"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ni",
        "title": "Ni"
      },
      {
        "command": "ni.ni",
        "title": "Install"
      },
      {
        "command": "ni.nr",
        "title": "Run"
      },
      {
        "command": "ni.nx",
        "title": "Execute"
      },
      {
        "command": "ni.nu",
        "title": "Upgrade"
      },
      {
        "command": "ni.nun",
        "title": "Uninstall"
      },
      {
        "command": "ni.nci",
        "title": "Clean Install"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ni",
          "group": "ni"
        },
        {
          "submenu": "ni.editor.context",
          "group": "ni"
        }
      ],
      "ni.editor.context": [
        {
          "command": "ni.ni",
          "group": "ni"
        },
        {
          "command": "ni.nr",
          "group": "ni"
        },
        {
          "command": "ni.nx",
          "group": "ni"
        },
        {
          "command": "ni.nu",
          "group": "ni"
        },
        {
          "command": "ni.nun",
          "group": "ni"
        },
        {
          "command": "ni.nci",
          "group": "ni"
        }
      ]
    },
    "submenus": [
      {
        "id": "ni.editor.context",
        "label": "Ni Commands"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsup",
    "watch": "tsup --watch",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.73.0",
    "execa": "^6.1.0",
    "ni": "^0.0.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
